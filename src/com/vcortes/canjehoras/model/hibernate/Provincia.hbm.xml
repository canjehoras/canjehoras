<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.vcortes.canjehoras.model.hibernate">
	<class name="Provincia" table="PROVINCIA">
		<id name="id" type="java.lang.Long">
			<column name="ID" precision="22" scale="0" />
			<generator class="native" />
		</id>
		<property name="codigo" column="CODIGO" type="string" not-null="false" />
		<property name="descripcion" column="DESCRIPCION" type="string" not-null="false" />
	</class>
</hibernate-mapping>