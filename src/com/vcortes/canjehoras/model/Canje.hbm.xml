<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.vcortes.canjehoras.model">
	<class name="Canje" table="CANJE">
		<id name="id" type="java.lang.Long">
			<column name="ID" precision="22" scale="0" />
			<generator class="native" />
		</id>
		<property name="fecha" type="java.util.Date">
            <column name="FECHA" length="19" not-null="true" />
        </property>
		<property name="hora_inicio" type="string">
            <column name="HORA_INICIO" length="5" not-null="true" />
        </property>
        <property name="hora_fin" type="string">
            <column name="HORA_FIN" length="5" not-null="true" />
        </property>
		<many-to-one column="ID_USUARIO" class="Usuario" name="usuario" lazy="proxy" cascade="all"/>
		<many-to-one column="ID_TRUEQUE" class="Trueque" name="trueque" lazy="proxy" cascade="all"/>
		<many-to-one column="ID_AGENDA" class="Agenda" name="agenda" lazy="proxy" cascade="all"/>
	</class>
</hibernate-mapping>